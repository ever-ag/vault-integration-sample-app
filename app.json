{
  "name": "Vault Integration Example",
  "description": "A barebones Node.js app that shows how to create a basic Vault Integration",
  "repository": "https://github.com/vaultdairy/vault-integration-sample-app",
  "logo": "https://node-js-sample.herokuapp.com/node.png",
  "keywords": ["node", "express", "static"],
  "env": {
    "PRIVATE_KEY": {
      "description": "The private key used to sign JWTs",
      "value": "-----BEGIN RSA PRIVATE KEY----------END RSA PRIVATE KEY-----"
    },
    "INTEGRATION_KEY_ID": {
      "description": "The ID of the key provided in the PRIVATE KEY field.",
      "value": "5"
    },
    "INTEGRATION_ID": {
        "description": "The integration ID of this integration",
        "value": "UPDATE_ME"
    },
    "VAULT_HOST": {
        "description": "The Vault API endpoint to use.",
        "value": "https://api.vaultdairy.com"
    }
  }
}